{
  "name": "shop-management-system",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "shop": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "ts-node prisma/seed.ts",
