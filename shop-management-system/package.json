    "migrate": "tsx src/migrate.ts"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@tensorflow/tfjs": "^4.22.0",
    "axios": "^1.7.9",
    "drizzle-orm": "^0.38.2",
    "i": "^0.3.7",
    "iron-session": "^8.0.4",
    "jose": "^5.9.6",
    "json2csv": "^6.0.0-alpha.2",
    "mongoose": "^8.9.1",
    "msnodesqlv8": "^4.4.0",
    "mssql": "^11.0.1",
    "next": "15.0.1",
    "node-fpgrowth": "^1.2.1",
    "npm": "^10.9.0",
    "papaparse": "^5.4.1",
    "pg": "^8.13.1",
    "prisma": "^5.21.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/json2csv": "^5.0.7",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.17.6",
